          <script type="text/javascript">
            window.onload = function () {
                CanvasJS.addColorSet("blueShades",
                [
                    "#004D93",
                    "#6199CB",
                    "#4284BF"
                ]);
              var chart2 = new CanvasJS.Chart("chartContainer2",
              {
                colorSet:"blueShades",
                animationEnabled: true,
                data: [
                {
                 type: "doughnut",
                 dataPoints: [
                 {  y: 12, indexLabel: "Download Sales" },
                 {  y: 20, indexLabel: "Mail-Order Sales" },
                 {  y: 30, indexLabel: "In-Store Sales" },
                 ]
               }
               ]
             });
          
              chart2.render();
            }
